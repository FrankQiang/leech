nethack on

# Emacs keybindings on C-a [
markkeys "h=^B:l=^F:$=^E"

caption always "%{= kw}%-w%{= gW}%n %t%{-}%+w %-= %{= r}%H%{-} %{= g}%l%{-} %D %Y-%m-%d %c:%s"

bind c screen 1
bind 0 select 10

screen -t torrent 1 rtorrent
screen -t bondage 2 bondage -d -w ~/utor/
screen -t utor    3 utor/torrent-announce
select 0
detach
